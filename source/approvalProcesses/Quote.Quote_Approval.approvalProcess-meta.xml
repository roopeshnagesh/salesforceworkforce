    <approvalStep>
        <allowDelegate>false</allowDelegate>
        <approvalActions>
            <action>
                <name>Quote_Approved_by_VP_Sales</name>
                <type>Alert</type>
            </action>
        </approvalActions>
        <assignedApprover>
            <approver>
                <name>john.greenstein@bluescape.com</name>
                <type>user</type>
            </approver>
            <whenMultipleApprovers>Unanimous</whenMultipleApprovers>
        </assignedApprover>
        <description>Approved through JG - per KB and MT 1.21.2020</description>
        <label>Sales Approval Step</label>
        <name>Sales_Approval_Step</name>
        <rejectionActions>
            <action>
                <name>Quote_Rejected_by_VP_Sales</name>
                <type>Alert</type>
            </action>
        </rejectionActions>
    </approvalStep>
    <approvalStep>
        <allowDelegate>true</allowDelegate>
        <assignedApprover>
            <approver>
                <name>kevin.beals@bluescape.com</name>
                <type>user</type>
            </approver>
            <whenMultipleApprovers>FirstResponse</whenMultipleApprovers>
        </assignedApprover>
        <label>Finance Approval Step</label>
        <name>Finance_Approval_Step</name>
        <rejectBehavior>
            <type>RejectRequest</type>
        </rejectBehavior>
        <rejectionActions>
            <action>
                <name>Quote_Rejected_Email</name>
                <type>Alert</type>
            </action>
        </rejectionActions>
    </approvalStep>
    <description>Approvals are needed when:
-No criteria</description>
    <emailTemplate>Opportunity_Quote_Approval_Process/Quote_Approval_Email</emailTemplate>
    <enableMobileDeviceAccess>true</enableMobileDeviceAccess>
    <finalApprovalActions>
        <action>
            <name>Quote_Approved_Email</name>
            <type>Alert</type>
        </action>
        <action>
            <name>Quote_Status_Set_to_Approved</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>Set_Quote_Record_Type_to_Approved</name>
            <type>FieldUpdate</type>
        </action>
    </finalApprovalActions>
    <finalApprovalRecordLock>false</finalApprovalRecordLock>
    <finalRejectionActions>
        <action>
            <name>Quote_Status_Set_to_Rejected</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>Set_Quote_Record_Type_to_Draft</name>
            <type>FieldUpdate</type>
        </action>
    </finalRejectionActions>
    <finalRejectionRecordLock>false</finalRejectionRecordLock>
    <initialSubmissionActions>
        <action>
            <name>Notify_Kevin_of_new_Quote_for_Approval</name>
            <type>Alert</type>
        </action>
        <action>
            <name>Quote_Status_Set_to_In_Review</name>
            <type>FieldUpdate</type>
        </action>
    </initialSubmissionActions>
    <label>Quote Approval Process (01 21 2020)</label>
    <nextAutomatedApprover>
        <useApproverFieldOfRecordOwner>true</useApproverFieldOfRecordOwner>
        <userHierarchyField>Manager</userHierarchyField>
    </nextAutomatedApprover>
    <recallActions>
        <action>
            <name>Quote_Recalled_Email</name>
            <type>Alert</type>
        </action>
        <action>
            <name>Quote_Status_Set_to_Draft</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>Set_Quote_Record_Type_to_Draft</name>
            <type>FieldUpdate</type>
        </action>
    </recallActions>
    <recordEditability>AdminOrCurrentApprover</recordEditability>
