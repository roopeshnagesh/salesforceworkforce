ISPICKVAL(StageName, &quot;6-Negotiate&quot;), 
ISPICKVAL(StageName, &quot;7-Pending Booking&quot;),
ISPICKVAL(StageName, &quot;Closed Won&quot;),
ISPICKVAL(StageName, &quot;R1-Pending&quot;),
ISPICKVAL(StageName, &quot;R2-In Progress&quot;),
ISPICKVAL(StageName, &quot;R3-Negotiate&quot;)),
CreatedBy.UserRole.Name = &quot;Sales Development Representative&quot; 
)</formula>
