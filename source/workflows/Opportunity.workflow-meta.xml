    <rules>
        <fullName>Close Date Outdated</fullName>
        <active>true</active>
        <criteriaItems>
            <field>Opportunity.IsClosed</field>
            <operation>equals</operation>
            <value>False</value>
        </criteriaItems>
        <description>Send notification to Sales Ops and VP of Sales when a Close Date is past due</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
        <workflowTimeTriggers>
            <actions>
                <name>WF_Close_Date_in_Past</name>
                <type>Alert</type>
            </actions>
            <offsetFromField>Opportunity.CloseDate</offsetFromField>
            <timeLength>1</timeLength>
            <workflowTimeTriggerUnit>Days</workflowTimeTriggerUnit>
        </workflowTimeTriggers>
    </rules>
    <rules>
        <fullName>Closed Won Oppty</fullName>
        <actions>
            <name>Email_Execs_on_Closed_Won_Oppty</name>
            <type>Alert</type>
        </actions>
        <actions>
            <name>Assign_CSM</name>
            <type>Task</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>Opportunity.StageName</field>
            <operation>equals</operation>
            <value>Closed - Won,Closed Won / Request Install,Closed Won</value>
        </criteriaItems>
        <description>Any Opportunity that is set to &#39;Closed - Won&#39; / &#39;Closed Won / Request Install&#39;</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>Contraction Expiration Notification</fullName>
        <active>false</active>
        <criteriaItems>
            <field>Opportunity.StageName</field>
            <operation>equals</operation>
            <value>Closed Won</value>
        </criteriaItems>
        <description>Alerts sales team and management that a closed opportunity has a renewal date in 90s days for further discussion and opp creation</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
        <workflowTimeTriggers>
            <actions>
                <name>Renewal_Opportunity_Due_in_90_Days</name>
